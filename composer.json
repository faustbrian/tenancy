{
    "name": "cline/tenancy",
    "description": "Composable multi-tenancy for Laravel.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "multi-tenant",
        "multitenancy",
        "php",
        "tenancy"
    ],
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@cline.sh"
        }
    ],
    "require": {
        "php": "^8.5.0",
        "illuminate/console": "^10.0 || ^11.0 || ^12.0",
        "illuminate/contracts": "^10.0 || ^11.0 || ^12.0",
        "illuminate/database": "^10.0 || ^11.0 || ^12.0",
        "illuminate/events": "^10.0 || ^11.0 || ^12.0",
        "illuminate/http": "^10.0 || ^11.0 || ^12.0",
        "illuminate/queue": "^10.0 || ^11.0 || ^12.0",
        "illuminate/routing": "^10.0 || ^11.0 || ^12.0",
        "spatie/laravel-package-tools": "^1.92.7",
        "cline/variable-keys": "^1.2"
    },
    "require-dev": {
        "cline/coding-standard": "^1.5.6",
        "orchestra/testbench": "^10.8",
        "symfony/var-dumper": "^7.4.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Cline\\Tenancy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Tenancy": "Cline\\Tenancy\\Facades\\Tenancy"
            },
            "providers": [
                "Cline\\Tenancy\\TenancyServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": "vendor/bin/ecs check --fix",
        "refactor": "rector",
        "test": [
            "@test:lint",
            "@test:type-coverage",
            "@test:unit",
            "@test:types",
            "@test:refactor"
        ],
        "test:lint": "vendor/bin/ecs check",
        "test:refactor": "rector --dry-run",
        "test:type-coverage": "pest --parallel --min=80 --type-coverage",
        "test:types": "phpstan",
        "test:unit": "pest --parallel --min=80 --coverage"
    }
}
